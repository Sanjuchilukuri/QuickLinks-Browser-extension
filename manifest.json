{
  "manifest_version": 3,
  "name": "Quick Links",
  "version": "1.0",
  "description": "Save and manage your quick links easily.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/icon.jpeg"
  },
  "permissions": ["storage", "tabs"],
  "icons": {
    "48": "icons/icon.jpeg"
  }
}
